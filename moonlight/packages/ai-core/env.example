# Moonlight AI Core Environment Variables
# Copy this file to .env and fill in your values

# OpenRouter API
OPENROUTER_API_KEY=your_openrouter_api_key_here
DEFAULT_MODEL=google/gemini-2.0-flash-exp:free

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=moonlight
POSTGRES_PASSWORD=moonlight_secret
POSTGRES_DB=moonlight

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Voice Service (gRPC)
VOICE_SERVICE_HOST=localhost
VOICE_SERVICE_PORT=50051

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=true


